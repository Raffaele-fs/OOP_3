SELECT MAX(PRICE) AS MAX_PRICE
FROM meal
WHERE calories > 500;

SELECT AVG(calories) AS AVERAGE_CALORIES
FROM meal
WHERE price > 15;

SELECT *
FROM meal
HAVING price = (SELECT MAX(price) FROM meal);

SELECT *
FROM meals
HAVING price = (SELECT MAX(price) FROM meals);

SELECT *
FROM meal
HAVING price < (SELECT AVG(price) FROM meal) AND calories > 600;

